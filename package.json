{
  "name": "seek",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "A promisified XMLHttpRequest with an API close to fetch",
  "keywords": ["seek", "fetch", "xmlhttprequest", "request", "network", "ajax", "promise"],
  "homepage": "https://github.com/pauldijou/seek",
  "author": {
    "name": "Paul Dijou",
    "url": "http://pauldijou.fr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pauldijou/seek"
  },
  "bugs": {
    "url": "https://github.com/pauldijou/seek/issues"
  },
  "main": "lib/node.js",
  "browser": "lib/browser.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib",
    "dist": "rimraf dist && webpack --config webpack.config.js --colors",
    "prepublish": "npm run clean && npm run build",
    "start": "npm run dist && node ./example/server.js",
    "test": "karma start"
  },
  "dependencies": {
    "xmlhttprequest": "^1.7.0",
    "form-data": "1.0.0-rc3"
  },
  "devDependencies": {
    "webpack": "^1.12.0",
    "babel": "^5.8.21",
    "babel-loader": "^5.3.0",
    "rimraf": "^2.4.3",
    "express": "^4.13.3",
    "body-parser": "^1.14.0",
    "multer": "^1.0.5",
    "cors": "^2.7.1",
    "jasmine": "^2.3.0",
    "karma": "^0.13.0",
    "karma-jasmine": "^0.3.6",
    "karma-chrome-launcher": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-webpack": "^1.7.0",
    "phantomjs": "1.9.17",
    "node-libs-browser": ">= 0.4.0 <=0.6.0"
  }
}
